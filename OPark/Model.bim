        "partitions": [
          {
            "name": "Partition",
            "dataView": "full",
            "source": {
              "type": "m",
              "expression": [
                "let",
                "    Source = #\"SQL/192 168 26 26;OPARK_BI\",",
                "    report_sales_target = Source{[Schema=\"report\",Item=\"sales_target\"]}[Data],",
                "    #\"Replaced Value\" = Table.ReplaceValue(report_sales_target,\"ي\",\"ی\",Replacer.ReplaceText,{\"sales_user_fullname\"}),",
                "    #\"Changed Type\" = Table.TransformColumnTypes(#\"Replaced Value\",{{\"month\", Int64.Type}})",
                "in",
                "    #\"Changed Type\""
              ]
            }
          }
        ],
        "measures": [
          {
            "name": "001_sum_sales_target",
            "description": [
              "جمع تارگت فروشندگان",
              "با توجه به اینکه برای جلوگیری از اضافه شدن جدول برای تارگت های گیشه گروهی در قسمت ای دی یوزر ها برای مقادیر گیشه گروهی عبارت ",
              "G",
              "درج شده که با فیلتر کردن این داده های عدد تارگت مربوط به قرارداد سازمانی استخراج می گردد."
            ],
            "expression": [
              "",
              "CALCULATE (",
              "    SUM ( 'report sales_target'[sales_user_target] ),",
              "    FILTER ( 'report sales_target', 'report sales_target'[sales_user_ID] <> \"G\" )",
              ")",
              "--جمع تارگت فروشندگان با توجه به اینکه برای جلوگیری از اضافه شدن جدول برای تارگت های گیشه گروهی در قسمت ای دی یوزر ها برای مقادیر گیشه گروهی عبارت  G درج شده که با فیلتر کردن این داده های عدد تارگت مربوط به قرارداد سازمانی استخراج می گردد."
            ],
            "formatString": "#,#"
          },
          {
            "name": "002_sum_Sold_at_boxoffice",
            "description": "جمع تارگت تعداد نفرات در گیشه آزاد",
            "expression": [
              "",
              "CALCULATE (",
              "    SUM ( 'report sales_target'[sales_user_target] ),",
              "    USERELATIONSHIP ( 'bi DimDate'[Jalali_1], 'report sales_target'[date_target] ),",
              "    FILTER ( 'report sales_target', 'report sales_target'[sales_user_ID] = \"G\" )",
              ")",
              "--جمع تارگت تعداد نفرات در گیشه آزاد"
            ],
            "formatString": "#,#"
          }
        ]
      },

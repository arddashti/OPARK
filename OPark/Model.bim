        "measures": [
          {
            "name": "001_Number_of_entries",
            "description": "تعداد میهمانان ورودی به پارک دقیق برای ثبت نفرات",
            "expression": [
              "",
              "VAR EN_no =",
              "    CALCULATE (",
              "        DISTINCTCOUNT ( 'report Reception_Log'[ReceptionID] ),",
              "        FILTER (",
              "            'report Reception_Log',",
              "            'report Reception_Log'[IsNewWristBand] = \"ورود\"",
              "        )",
              "    )",
              "RETURN",
              "    IF ( ISBLANK ( EN_no ), 0, EN_no )",
              "--تعداد میهمانان ورودی به پارک دقیق برای ثبت نفرات"
            ],
            "formatString": "#,#"
          },
          {
            "name": "002_Number_of_entrances_to_the_station",
            "description": "تعداد میهمانان ورودی به ایستگاه ها",
            "expression": [
              "",
              "CALCULATE (",
              "    DISTINCTCOUNT ( 'report Reception_Log'[ReceptionID] ),",
              "    FILTER ( 'report Reception_Log', 'report Reception_Log'[IsStationLog] = TRUE )",
              ")",
              "--تعداد میهمانان ورودی به ایستگاه ها"
            ],
            "formatString": "#,#"
          },
          {
            "name": "003_01_The_amount_of_charge_used_at_the_stations",
            "description": [
              "مقدار شارژ مصرف شده در ایستگاه ها به غیر از ایستگاه ",
              "O Lounge"
            ],
            "expression": [
              "",
              "(",
              "    CALCULATE (",
              "        SUM ( 'report Reception_Log'[UsageAmount] ),",

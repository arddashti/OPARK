          {
            "name": "024_capitationl_amount_of_charge_used_at_the_stations_in_contract",
            "description": "سرانه مصرف شارژ در ایستگاه ها برای افراد وارد شده به ایستگاه ها",
            "expression": [
              "",
              "DIVIDE (",
              "    'report sales_contract'[023_total_amount_of_charge_used_at_the_stations_in_contract],",
              "    'report sales_contract'[022_total_Number_of_entrances_to_the_station_in_contract]",
              ")",
              "--سرانه مصرف شارژ در ایستگاه ها برای افراد وارد شده به ایستگاه ها"
            ],
            "formatString": "#,#"
          },
          {
            "name": "025_number_of_working_days",
            "description": "تعداد روزهای کاری انتخاب شده",
            "expression": [
              "",
              "VAR today_date = 1",
              "VAR last_week = 1",
              "VAR all_day =",
              "    DISTINCTCOUNT ( 'report Reception_Log'[date_search] )",
              "RETURN",
              "    IF (",
              "        SELECTEDVALUE ( 'in select_table_date_now'[persian select subject] ) = \"امروز\",",
              "        today_date,",
              "        IF (",
              "            SELECTEDVALUE ( 'in select_table_date_now'[persian select subject] ) = \"هفته گذشته\",",
              "            last_week,",
              "            all_day",
              "        )",
              "    )",
              "--تعداد روزهای کاری انتخاب شده"
            ],
            "formatString": "#,#"
          },
          {
            "name": "026_Average_Number_of_entries_in_contract",
            "description": "متوسط تعداد نفرات حاضر در پارک در هر روز",
            "expression": [
              "",
              "DIVIDE (",
              "    'report sales_contract'[003_total_Number_of_entries_in_contract],",
              "    'report sales_contract'[025_number_of_working_days]",
              ")",
              "--متوسط تعداد نفرات حاضر در پارک در هر روز"
            ],
            "formatString": "#,#"
          },
          {
            "name": "027_Average_total_income_in_contract",
            "description": "متوسط روزانه درآمد پارک در بازه انتخابی",
            "expression": [
              "",
              "DIVIDE (",
              "    'report sales_contract'[002_total_income_in_contract],",
              "    'report sales_contract'[025_number_of_working_days]",
              ")",
              "--متوسط روزانه درآمد پارک در بازه انتخابی"
            ],
            "formatString": "#,#"
          },
          {
            "name": "028_total_session_count",
            "description": "جمع تعداد جلسات",
            "expression": [
              "",
              "CALCULATE ( SUM ( 'report sales_contract'[total_session_count] ) )",
              "--جمع تعداد جلسات"
            ]
          },
          {
            "name": "029_Target percentage",
            "description": "درصد تحقق تارگت",
            "expression": [
              "",
              "VAR t = DIVIDE (",
              "    'report sales_contract'[028_total_session_count],",
              "    'report sales_target'[001_sum_sales_target]",
              ") * 100",
              "",
              "RETURN",
              "IF(ISBLANK(t),0,t)",
              "--درصد تحقق تارگت"
            ],
            "formatString": "0.00"
          },
          {
            "name": "030_total_Numbe_ expired_sessions",
            "description": "تعداد کل جلسات باطل شده",
            "expression": [
              "",
              "SUM ( 'report sales_contract'[no_deleted_session] )",
              "--تعداد کل جلسات باطل شده"
            ],
            "formatString": "#,#"
          },
          {
            "name": "031_total_session_count_net",
            "description": "تعداد کل جلسات منهای جلسات ابطال شده",
            "expression": [
              "",
              "'report sales_contract'[028_total_session_count] - 'report sales_contract'[030_total_Numbe_ expired_sessions]",
              "--تعداد کل جلسات منهای جلسات ابطال شده"
            ],
            "formatString": "#,#"
          },
          {
            "name": "032_sum_sales_forecast",
            "description": "جمع مبلغ پیش بینی شده برای هر قرارداد",
            "expression": [
              "",
              "IF (",
              "    ISBLANK ( CALCULATE ( SUM ( 'report sales_contract'[sales_forecast] ) ) ),",
              "    0,",
              "    CALCULATE ( SUM ( 'report sales_contract'[sales_forecast] ) )",
              ")",
              "--جمع مبلغ پیش بینی شده برای هر قرارداد"
            ],
            "formatString": "#,#"
          },
          {
            "name": "033_total_SumTicketPrice_in_contract",
            "description": [
              "مبلغ دریافت شده در گیشه",
              "به تفکیک قرارداد  ",
              "مربوط به میهمانان گروهی"
            ],
            "expression": [
              "",
              "VAR t = CALCULATE (",
              "    'in select_table_date_now'[012_SumTicketPrice_with_today],",
              "    USERELATIONSHIP ( 'report sales_contract'[TicketCardMainID], 'report Reception_Log'[TicketCardMainID] ),",
              "    FILTER (",
              "        'report Reception_Log',",
              "        'report Reception_Log'[TicketName] = \"گیشه گروهی\"",
              "    )",
              ")",
              "RETURN",
              "IF(ISBLANK(t),0,t)",
              "--مبلغ دریافت شده در گیشه به تفکیک قرارداد مربوط به میهمانان گروهی"
            ],
            "formatString": "#,#"
          },
          {
            "name": "034_Target percentage_box_office",
            "description": "درصد تحقق تارگت در گیشه آزاد",
            "expression": [
              "",
              "VAR Number_of_entries_in_boxoffice =",
              "    CALCULATE (",
              "        'report Reception_Log'[001_Number_of_entries],",
              "        FILTER (",
              "            'report Reception_Log',",
              "            'report Reception_Log'[TicketName] = \"گیشه آزاد\"",
              "        ),",
              "FILTER('bi DimDate','bi DimDate'[Counter (#1)] <> 0)",
              "    )",
              "",
              "",
              "",
              "VAR RST = CALCULATE('report sales_target'[002_sum_Sold_at_boxoffice] , FILTER('bi DimDate','bi DimDate'[Counter (#1)] <> 0))",
              "",
              "RETURN",
              "    DIVIDE (",
              "        Number_of_entries_in_boxoffice,RST",
              "        ",
              "    ) * 100",
              "--درصد تحقق تارگت در گیشه آزاد"
            ],
            "formatString": "#,#"
          },
          {
            "name": "035_Number_of_entries_in_boxoffice",
            "description": "جمع نفرات در گیشه آزاد",
            "expression": [
              "",
              "CALCULATE (",
              "    'report Reception_Log'[001_Number_of_entries],",
              "    FILTER (",
              "        'report Reception_Log',",
              "        'report Reception_Log'[TicketName] = \"گیشه آزاد\"",
              "    )",
              ")",
              "--جمع نفرات در گیشه آزاد"
            ],
            "formatString": "#,#"
          },
          {
            "name": "036",
            "expression": " 'report sales_contract'[031_total_session_count_net]-'report sales_contract'[003_total_Number_of_entries_in_contract]"
          }
        ]
      },
